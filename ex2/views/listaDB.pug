extends layout
block content
  .w3-card-4
    header.w3-container.w3-teal
      h1 Exames médicos desportivos
    .w3-container
      table.w3-table-all
        tr
          th Data
          th Nome
          th Género
          th Idade 
          th Resultado
        
        each exame in exames
          tr(onclick='changePage(\"' + exame._id + '\")')
            td= exame.dataEMD
            td #{exame.nome.primeiro + " " + exame.nome.último}
            td= exame.género
            td= exame.idade
            td= exame.resultado
            td
              button.w3-btn.w3-round.w3-indigo.w3-margin-right 
                a(href="#") C
              button.w3-btn.w3-round.w3-indigo.w3-margin-right  
                a(href="/retrieveList/" + exame._id) R
              button.w3-btn.w3-round.w3-indigo.w3-margin-right  
                a(href="#") U 
              button.w3-btn.w3-round.w3-indigo.w3-margin-right  
                a(href="#") D
              
    footer.w3-container.w3-teal
      h5 Generated by _____
